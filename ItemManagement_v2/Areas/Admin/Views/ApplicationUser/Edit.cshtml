@model ApplicationUserEdit


<h2> Edit @Model.UserName </h2>

<form action="/admin/applicationUser/edit" method="POST">
    <div class="form-row">
        <div class="col-md-6"> 

        <input type="hidden" value="@Model.Id" name="Id" />
        <div class="form-group col-md-6">
            <label for="Username"> Username </label>
            <input type="text" class="form-control" id="Username" name="UserName" placeholder="Username" value="@Model.UserName" />
        </div>
        <div class="form-group col-md-6">
            <label for="Email"> Tag </label>
            <input type="text" class="form-control" id="Email" name="Email" placeholder="Email" value="@Model.Email" />
        </div>
    </div>
  </div>
    

    <button type="submit" class="btn btn-success"> Update </button>
    <a class="btn btn-danger" href="/home"> Cancel</a>
</form>

